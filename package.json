{
  "name": "personal-ai-organizer",
  "version": "1.0.0",
  "description": "AI-powered personal organization system",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client:3001\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "client:3001": "cd client && npm run start:3001",
    "client:3002": "cd client && npm run start:3002",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": ["ai", "organization", "personal", "automation"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 